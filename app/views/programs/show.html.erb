<div class="row">
  <div class="page-header col-md-12">
    <h1><%= @program.name %></h1>
  </div>
</div>
<div class="row">
  <div class="col-md-8">
    <% @program.wods.each do |w| %>
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title"><%= w.name %></h3>
        </div>
        <div class="panel-body">
          <%= w.description %>
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-3 offset-md-1">
    <h4>Other <%= @program.name %> members</h4>
    <ul class="list-group">
      <% @program.users.each do |u| %>
        <li class="list-group-item"><%= u.first_name + " " + u.last_name %></li>
      <% end %>
    </ul>
  </div>
</div>
